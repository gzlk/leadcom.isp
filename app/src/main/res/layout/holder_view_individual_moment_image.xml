<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="horizontal">

    <com.leadcom.android.isp.lib.view.ImageDisplayer
        android:id="@+id/ui_holder_view_individual_moment_image_1"
        android:layout_width="@dimen/ui_base_image_line_image_size"
        android:layout_height="@dimen/ui_base_image_line_image_size"
        android:layout_marginEnd="@dimen/ui_static_dp_5"
        android:visibility="gone"
        app:id_image_scale_type="centerCrop"
        app:id_image_src="@mipmap/img_image_default"
        app:id_image_type="normal" />

    <com.leadcom.android.isp.lib.view.ImageDisplayer
        android:id="@+id/ui_holder_view_individual_moment_image_2"
        android:layout_width="@dimen/ui_base_image_line_image_size"
        android:layout_height="@dimen/ui_base_image_line_image_size"
        android:layout_marginEnd="@dimen/ui_static_dp_5"
        android:visibility="gone"
        app:id_image_scale_type="centerCrop"
        app:id_image_src="@mipmap/img_image_default"
        app:id_image_type="normal" />

    <com.leadcom.android.isp.lib.view.ImageDisplayer
        android:id="@+id/ui_holder_view_individual_moment_image_3"
        android:layout_width="@dimen/ui_base_image_line_image_size"
        android:layout_height="@dimen/ui_base_image_line_image_size"
        android:layout_marginEnd="@dimen/ui_static_dp_5"
        android:visibility="gone"
        app:id_image_scale_type="centerCrop"
        app:id_image_src="@mipmap/img_image_default"
        app:id_image_type="normal" />
</LinearLayout>